БюджетРоялти:
Load
	//[სეგმენტი] as [კონტრაგენტის ჯგუფი (ბიუჯეტისთვის)],
    ApplyMap('MapРегистрСведенийДанныеДляРасчетаРоялти', floor(monthend(Date#([Date],'YYYY-MM-DD'))),'00') as [ფაქტიური მონაცემები (როიალტის გეგმა)],
    floor(monthend(Date#([Date],'YYYY-MM-DD'))) as [RoyaltyBudgetDate],
    floor(monthend(Date#([Date],'YYYY-MM-DD'))) & '|' & 0 as [DateForConnect],
    Sum([Value]) as [ბიუჯეტი (როიალტის გეგმა)]
Resident RoyaltyPlanFromSheets
//where floor(monthend(Date#([Date],'YYYY-MM-DD'))) >= YearStart($(vNow))
group by 
	floor(monthend(Date#([Date],'YYYY-MM-DD')))
;

drop table RoyaltyPlanFromSheets;

